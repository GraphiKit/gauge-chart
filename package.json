{
  "name": "gauge-chart",
  "version": "1.0.0",
  "description": "Simple and lightweight vector gauge chart",
  "main": "dist/index.min.js",
  "module": "dist/index.mjs",
  "exports": {
    "require": "./dist/index.min.js",
    "import": "./dist/index.mjs",
    "default": "./dist/index.mjs"
  },
  "scripts": {
    "build": "terser src/index.mjs -o dist/index.mjs --compress --mangle && terser src/index.cjs -o dist/index.cjs --compress --mangle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphikit/gauge-chart.git"
  },
  "keywords": [
    "gauge",
    "chart",
    "light"
  ],
  "author": "jguillaumesio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/graphikit/gauge-chart/issues"
  },
  "homepage": "https://github.com/graphikit/gauge-chart#readme",
  "devDependencies": {
    "terser": "^5.39.0"
  }
}
